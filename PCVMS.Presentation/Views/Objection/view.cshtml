@using PCVMS.Presentation.Helpers;
@inject ICacheHelper obj;
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var Localizer = await obj.GetAllResource();

}

@model PCVMS.Model.Web.PCVMS_Project_Model

<div class="main-card mb-3 card">
    <div class="card-header-custom  bg-moi-green">
        <i class="header-icon pe-7s-pen icon-gradient bg-white"> </i>
        @Localizer["property owner objection approval"]
    </div>

    <div class="card-body">
        <div class="form-row">

            <div class="col-md-4">
                <div class="position-relative form-group">
                    <label for="RoyalDecree">
                        @Localizer["project name"]

                    </label>
                    <div class="input-group">
                        <select id="ealist" class="form-control">
                            <option>@Localizer["batna road"]</option>
                            <option>@Localizer["batna road"]</option>
                            <option>@Localizer["rasal jinz tortile resort"]</option>
                            <option>@Localizer["rasal jinz tortile resort"]</option>
                        </select>
                        <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-bookmarks btn-icon-wrapper"> </i></span></div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="position-relative form-group">
                    <label for="ProjectName">@Localizer["property name"]</label>
                    <div class="widget-content-left">
                        <div class="input-group">
                            <select id="ealist" class="form-control">
                                <option selected>@Localizer["select property name"]</option>
                                <option>@Localizer["building no 204"]</option>
                                <option>@Localizer["barka farm house"]</option>

                            </select>
                            <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-speaker btn-icon-wrapper"> </i></span></div>

                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="position-relative form-group">
                    <label for="exampleEmail2" id="lbl_stage3_name">@Localizer["governorates"]</label>
                    <div class="input-group">
                        @*@Html.DropDownListFor(model => model.GovernetId, Model.GovernoratesList, "Select Governorate", new { @class = "form-control" })*@
                        <select class="form-control"><option selected>Muscat</option><option>Dhofar</option></select>
                        <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-way btn-icon-wrapper"> </i></span></div>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <div class="d-block text-right card-footer">

        <button id="btnClear" name="btnClear" class="btn-icon btn-shadow btn-dashed btn btn-moi-gold"><i class="pe-7s-trash btn-icon-wrapper"> </i>@Localizer["clear"]</button>
        <button id="SearchTransaction" name="SearchTransaction" class="btn-icon btn-shadow btn-dashed btn btn-alternate block-page-btn-example-3">
            <i class="pe-7s-search btn-icon-wrapper"> </i>@Localizer["search"]
        </button>
    </div>
    <table id="example2" style="width: 100%;" class="table table-hover table-striped table-bordered">
        <thead>
            <tr>

                <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-magic-wand btn-icon-wrapper"> </i> @Localizer["project name"]</th>
                <th class="bg-moi-gold" style="text-align:center;"><i class="pe-7s-magic-wand btn-icon-wrapper"> </i> @Localizer["property name"]</th>
                <th class="bg-moi-gold" style="text-align:center;"><i class="pe-7s-magic-wand btn-icon-wrapper"> </i> @Localizer["objection date"]</th>
                <th class="bg-moi-gold" style="text-align:center;"><i class="pe-7s-magic-wand btn-icon-wrapper"> </i> @Localizer["objection approval status"]</th>
                <th class="bg-moi-gold" style="text-align:center;"><i class="pe-7s-magic-wand btn-icon-wrapper"> </i> @Localizer["action"]</th>
            </tr>
        </thead>
        <tbody>
            <tr>

                <td class="text-center">al batinah coastal road</td>
                <td class="text-center">Building No 204</td>
                <td class="text-center">11/11/2021</td>
                <td class="text-center">Pending</td>
                <td class="text-center" id='tdAction' name='tdAction'><div class='position-relative'><i class="metismenu-icon lnr-book" style="cursor: pointer;margin-left: 10px;" title="view"></i></div></td>
            </tr>
            <tr>

                <td class="text-center">salalah road</td>
                <td class="text-center">Building No 205</td>

                <td class="text-center">21/11/2021</td>
                <td class="text-center">Pending</td>
                <td class="text-center" id='tdAction' name='tdAction'><div class='position-relative'><i class="metismenu-icon lnr-book" style="cursor: pointer;margin-left: 10px;" title="view"></i></div></td>
            </tr>

        </tbody>
    </table>

</div>
<div id="smartwizard-objection-approval-view" class="main-card mb-3 card">
    <div class="card-header-custom  bg-moi-green">
        <i class="header-icon pe-7s-pen icon-gradient bg-royal"> </i>

        @Localizer["property owner objection registration"]
    </div>
    <div class="card-body">
        <div class="form-row">

            <div class="col-md-4">
                <div class="position-relative form-group">
                    <label for="RoyalDecree">
                        Property Owner Name

                    </label>
                    <div class="input-group">
                        <input readonly="readonly" name="txt_stage3_vehicletype" value="Abdul Malik" id="RoyalDecree" type="text" class="form-control">
                        <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-bookmarks btn-icon-wrapper"> </i></span></div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="position-relative form-group">
                    <label for="ProjectName">@Localizer["national id"]</label>
                    <div class="widget-content-left">

                        <div class="input-group">
                            <input readonly="readonly" name="txt_stage3_vehicletype" value="104020634" id="RoyalDecree" type="text" class="form-control">
                            <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-bookmarks btn-icon-wrapper"> </i></span></div>
                        </div>
                    </div>

                </div>
            </div>
            <div class="col-md-4">
                <div class="position-relative form-group">
                    <label for="RoyalDecree">
                        @Localizer["project name"]

                    </label>
                    <div class="input-group">
                        <select id="ealist" class="form-control">
                            <option>@Localizer["batna road"]</option>
                            <option>@Localizer["batna road"]</option>
                            <option>@Localizer["rasal jinz tortile resort"]</option>
                            <option>@Localizer["rasal jinz tortile resort"]</option>
                        </select>
                        <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-bookmarks btn-icon-wrapper"> </i></span></div>
                    </div>
                </div>
            </div>

        </div>


        <div class="form-row">
            <div class="col-md-4">
                <div class="position-relative form-group">
                    <label for="exampleEmail2" id="lbl_stage3_name"> @Localizer["objection type"] </label>

                    <div class="input-group">
                        <select id="ealist" class="form-control">
                            <option>@Localizer["property count"] 1</option>
                            <option>@Localizer["property compensation"] 2</option>
                        </select>
                        <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-speaker btn-icon-wrapper"> </i></span></div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="position-relative form-group">
                    <label for="ProjectName">@Localizer["property name"]</label>
                    <div class="widget-content-left">
                        <div class="input-group">
                            <select id="ealist" class="form-control">
                                <option selected>@Localizer["select property name"]</option>
                                <option>@Localizer["building no 204"]</option>
                                <option>@Localizer["barka farm house"]</option>

                            </select>
                            <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-speaker btn-icon-wrapper"> </i></span></div>

                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="position-relative form-group">
                    <label for="ProjectStartDate">@Localizer["objection date"] </label>

                    <div class="input-group">
                        <input name="complaintDate" id="complaintDate" type="text" class="form-control">
                        <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-date btn-icon-wrapper"> </i></span></div>
                    </div>
                </div>
            </div>

        </div>

        <div class="form-row">
            <div class="col-md-12">
                <div class="position-relative form-group">
                    <label for="exampleAddress">@Localizer["statement of owner"]</label>


                    <div class="input-group">
                        <input name="txtremark" id="txtremark" placeholder="please enter here..." type="text" class="form-control">
                        <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-notebook btn-icon-wrapper"> </i></span></div>
                    </div>
                </div>
            </div>
        </div>

        <div class="form-row">
            <div class="col-md-12">
                <div class="position-relative form-group">
                    <label for="exampleAddress">@Localizer["remark"]</label>


                    <div class="input-group">
                        <input name="txtremark" id="txtremark" placeholder="please enter here..." type="text" class="form-control">
                        <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-notebook btn-icon-wrapper"> </i></span></div>
                    </div>
                </div>
            </div>
        </div>


    </div>

</div>
<div class="main-card mb-3 card">
    <div class="card-header-custom  bg-moi-green" style="margin-bottom:10px;">
        <i class="header-icon pe-7s-pen icon-gradient bg-royal"> </i>
        @Localizer["attached objection document"]
    </div>

    <div class="card-body">
        <table style="width: 100%;" id="tbl-ProjectDocument" name="tbl-ProjectDocument" class="table table-hover table-striped table-bordered">
            <thead>
                <tr>
                    <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-id btn-icon-wrapper"> </i> @Localizer["s. no"]</th>
                    <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-album btn-icon-wrapper"> </i> @Localizer["objection document"]</th>
                    <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-magic-wand btn-icon-wrapper"> </i> @Localizer["action"]</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td class="text-center">1</td>
                    <td class="text-center">Ojection Letter from Poperty Owner</td>
                    <td class="text-center" id='tdAction' name='tdAction'><div class='position-relative'><i class="metismenu-icon lnr-book" style="cursor: pointer;margin-left: 10px;" title="view"></i></div></td>
                </tr>
            </tbody>
        </table>
    </div>

</div>

<script src="~/AppJs/Project.js"></script>
