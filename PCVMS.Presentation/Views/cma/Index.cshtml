    <link href="~/fontello.css" rel="stylesheet">

    @if (browserLang.ToLower() == "en-us")
    {
        <link href="~/main.87c0748b313a1dda75f5cma.css" rel="stylesheet" />
        <link href="~/css/Custom-en.css" rel="stylesheet" />
    }
    else

