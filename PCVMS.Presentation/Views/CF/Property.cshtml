                                        <button class="btn-shadow btn-wide btn btn-danger btn-lg">Reject</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="step-4">
                    <div class="no-results">
                        <div class="swal2-icon swal2-success swal2-animate-success-icon">
                            <div class="swal2-success-circular-line-left" style="background-color: rgb(255, 255, 255);"></div>
                            <span class="swal2-success-line-tip"></span>
                            <span class="swal2-success-line-long"></span>
                            <div class="swal2-success-ring"></div>
                            <div class="swal2-success-fix" style="background-color: rgb(255, 255, 255);"></div>
                            <div class="swal2-success-circular-line-right" style="background-color: rgb(255, 255, 255);"></div>
                        </div>
                        <div class="results-subtitle mt-4">@Localizer["finished"]!</div>
                        <div class="results-title" id="last-stage-message" name="last-stage-message">@Localizer["click the finish button to complete the process"]!</div>
                        <div class="mt-3 mb-3"></div>
                        <div class="text-center">
                            <button id="Finish" class="btn-shadow btn-wide btn btn-success btn-lg" id="btn-finish" name="btn-finish">@Localizer["finish"]</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="divider"></div>
        <div class="clearfix">
            <button type="button" id="reset-btn" class="btn-shadow float-left btn btn-link">Reset</button>
            <button type="button" id="next-btn" class="btn-shadow btn-wide float-right btn-pill btn-hover-shine btn btn-success">Next</button>
            <button type="button" id="prev-btn" class="btn-shadow float-right btn-wide btn-pill mr-3 btn btn-outline-secondary">Previous</button>
        </div>
    </div>
</div>
<script src="~/AppJs/Project.js"></script>
