
@using PCVMS.Presentation.Helpers;
@inject ICacheHelper obj;

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var Localizer = await obj.GetAllResource();
}
<div class="main-card mb-3 card">
    <div class="card-header-custom  bg-moi-green">
        <i class="header-icon pe-7s-pen icon-gradient bg-white"> </i>
         @Localizer["valuation foundation registration"]
    </div>

    <div class="card-body">
        <div class="form-row">
            <div class="col-md-4">
                <div class="position-relative form-group">
                    <label for="RoyalDecree">
                        @Localizer["valuation foundation name"]

                    </label>
                    <div class="input-group">
                        <select id="ealist" class="form-control">
                            <option selected>Select Value</option>
                            <option>Valuation Foundation 2021</option>
                            <option>Valuation Foundation 2010</option>

                        </select>
                        <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-bookmarks btn-icon-wrapper"> </i></span></div>
                    </div>
                </div>
                </div>
                <div class="col-md-4">
                    <div class="position-relative form-group">
                        <label for="ProjectName">@Localizer["event status"]</label>
                        <div class="widget-content-left">
                            <div class="input-group">
                                <select id="ealist" class="form-control">
                                    <option selected>@Localizer["select value"]</option>
                                    <option>Active</option>
                                    <option>de-active</option>

                                </select>
                                <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-speaker btn-icon-wrapper"> </i></span></div>

                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-md-2">
                    <div class="position-relative form-group">
                        <label for="ProjectName" style="visibility:hidden">@Localizer["cr"]</label>
                        <div class="widget-content-left">
                            <div class="input-group">
                                <button id="SearchTransaction" name="SearchTransaction" class="btn-icon btn-shadow btn-dashed btn btn-alternate block-page-btn-example-3">
                                    <i class="pe-7s-search btn-icon-wrapper"> </i>@Localizer["search"]
                                </button>

                            </div>
                        </div>

                    </div>
                </div>
            </div>

            <table style="width: 100%;" id="example2" name="tbl-resource-history" class="table table-hover table-striped table-bordered">
                <thead>
                    <tr>
                        <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-magic-wand btn-icon-wrapper"> </i> @Localizer["s. no"]</th>
                        <th class="bg-moi-gold" style="text-align:center;"><i class="pe-7s-magic-wand btn-icon-wrapper"> </i> @Localizer["event name"]</th>
                        <th class="bg-moi-gold" style="text-align:center;"><i class="pe-7s-magic-wand btn-icon-wrapper"> </i> @Localizer["status"]</th>
                        <th class="bg-moi-gold" style="text-align:center;"><i class="pe-7s-magic-wand btn-icon-wrapper"> </i> @Localizer["action"]</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>

                        <td class="text-center">1</td>
                        <td>Valuation Foundation 2021</td>
                        <td>Active</td>
                        <td class="text-center" id='tdAction' name='tdAction'><div class='position-relative'><i class="metismenu-icon lnr-pencil" style="cursor: pointer;margin-left: 10px;" title="edit"></i><i class="metismenu-icon lnr-trash" style="cursor: pointer;margin-left: 10px;" title="delete"></i></div></td>
                    </tr>
                    <tr>

                        <td class="text-center">2</td>
                        <td>Valuation Foundation 2010</td>
                        <td>Deactive</td>
                        <td class="text-center" id='tdAction' name='tdAction'><div class='position-relative'><i class="metismenu-icon lnr-pencil" style="cursor: pointer;margin-left: 10px;" title="edit"></i><i class="metismenu-icon lnr-trash" style="cursor: pointer;margin-left: 10px;" title="delete"></i></div></td>
                    </tr>
                </tbody>
            </table>
    </div>


</div>

<div id="ProjectDiv" class="main-card mb-3 card">
    <div class="card-header-custom  bg-moi-green">
        <i class="header-icon pe-7s-pen icon-gradient bg-royal"> </i>

        @Localizer["valuation foundation"]
    </div>
    <div class="card-body">
        <div class="form-row">
            <div class="col-md-6">
                <div class="position-relative form-group">
                    <label for="RoyalDecree">
                        @Localizer["valuation foundation name"]

                    </label>
                    <div class="input-group">
                        <input name="disputeEndDate" id="disputeEndDate" type="text" class="form-control">
                        <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-date btn-icon-wrapper"> </i></span></div>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="position-relative form-group">
                    <label for="ProjectStartDate">@Localizer["start date"] </label>

                    <div class="input-group">
                        <input name="disputeStartDate" id="disputeStartDate" type="text" class="form-control">
                        <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-date btn-icon-wrapper"> </i></span></div>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="position-relative form-group">
                    <label for="ProjectStartDate">@Localizer["end date"] </label>

                    <div class="input-group">
                        <input name="disputeEndDate" id="disputeEndDate" type="text" class="form-control">
                        <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-date btn-icon-wrapper"> </i></span></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="form-row">
            <div class="col-md-9">
                <div class="position-relative form-group">
                    <label for="exampleAddress">@Localizer["remark"]</label>
                    <div class="input-group">
                        <input name="txtremark" id="txtremark" placeholder="enetr here..." type="text" class="form-control">
                        <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-notebook btn-icon-wrapper"> </i></span></div>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="position-relative form-group">
                    <label for="exampleAddress">@Localizer["current status"]</label>
                    <div class="input-group">
                        <input name="txtremark" id="txtremark" placeholder="active" type="text" class="form-control">
                        <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-notebook btn-icon-wrapper"> </i></span></div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="main-card mb-3 card">
        <div class="card-header-custom  bg-moi-green" style="margin-bottom:10px;">
            <i class="header-icon pe-7s-pen icon-gradient bg-royal"> </i>
            Upload Valuation Foundation Excel Sheet
        </div>

        <div class="card-body">
            <div class="form-row">

                <div class="col-md-5">
                    <div class="position-relative form-group">
                        <label for="exampleEmail2">@Localizer["please, upload latest valuation foundation"]</label>
                    </div>
                </div>
                <div class="col-md-2">
                    <div class="position-relative form-group">
                        <img name="email" id="ProjectImage1" height="18" width="24">
                        <input type="hidden" id="Image1" height="18" width="24">

                    </div>
                </div>
                <div class="col-md-5">
                    <div class="position-relative form-group">
                        <input id="ProjectFile1" name="ProjectFile1" accept=".jpg,.jpeg" type="file" size="1" multiple onchange="uploadFiles(this);" />

                    </div>
                </div>

            </div>

        </div>
        <div class="card-body">
            <table style="width: 100%;" id="tbl-ProjectDocument" name="tbl-ProjectDocument" class="table table-hover table-striped table-bordered">
                <thead>
                    <tr>
                        <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-id btn-icon-wrapper"> </i> @Localizer["s. no"]</th>
                        <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-album btn-icon-wrapper"> </i> @Localizer["file name"]</th>
                        <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-magic-wand btn-icon-wrapper"> </i> @Localizer["action"]</th>
                    </tr>
                </thead>
                <tbody>

                    <tr>
                        <td>1</td>
                        <td>Valuation Foundation 2021</td>
                        <td class="text-center" id='tdAction' name='tdAction'><div class='position-relative'><i class="metismenu-icon lnr-pencil" style="cursor: pointer;margin-left: 10px;" title="edit"></i><i class="metismenu-icon lnr-trash" style="cursor: pointer;margin-left: 10px;" title="delete"></i></div></td>

                    </tr>
                </tbody>
            </table>
        </div>

    </div>
    <div class="d-block text-right card-footer">

        <button id="btnClear" name="btnClear" class="btn-icon btn-shadow btn-dashed btn btn-moi-gold"><i class="pe-7s-trash btn-icon-wrapper"> </i>@Localizer["clear"]</button>
        <button id="SearchTransaction" name="SearchTransaction" class="btn-icon btn-shadow btn-dashed btn btn-alternate block-page-btn-example-3">
            <i class="pe-7s-search btn-icon-wrapper"> </i>@Localizer["save"]
        </button>
    </div>
</div>

<!-- Disable tap highlight on IE -->
<meta name="msapplication-tap-highlight" content="no">

