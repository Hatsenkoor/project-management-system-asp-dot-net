                                <label for="PropertyRemark">@Localizer["remark"]</label>

                                <div class="input-group">
                                    <input name="PropertyRemark" id="PropertyRemark" placeholder="any remark, please enter here..." type="text" class="form-control">
                                    <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-notebook btn-icon-wrapper"> </i></span></div>
                                </div>
                            </div>
                        </div>
                    </div>
