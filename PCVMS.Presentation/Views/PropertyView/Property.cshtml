                                </div>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="position-relative form-group">
                                <label for="PropertyLandline">@Localizer["landline"]</label>

                                <div class="input-group">
                                    <input readonly="readonly" name="PropertyLandline" value="24138403" id="PropertyLandline" type="text" class="form-control">
