@using PCVMS.Presentation.Helpers;
@inject ICacheHelper obj;
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var Localizer = await obj.GetAllResource();

}

@model PCVMS.Model.Web.PCVMS_Project_Model

    <div class="main-card mb-3 card">
        <div class="row">
            <div class="col-lg-12 col-md-12">
               
                    <div class="card-header  bg-moi-blue-light">
                        <i class="header-icon pe-7s-pen icon-gradient bg-white"> </i>
                        <h1>@Localizer["project management module"]</h1>
                    </div>
            

            </div>

        </div>

        <div class="form-row">
            <div class="col-md-3"></div>
            <div class="col-md-6">
                <div class="position-relative form-group">
                    <label for="exampleEmail2" id="lbl_stage3_name"><span class="text-danger">*</span>@Localizer["select project name"]</label>
                    <div class="input-group">
                        @Html.DropDownListFor(model => model.ProjectId, Model.ProjectList, "al batinah coastal road", new { @class = "form-control" })
                        <div class="input-group-append"><span class="input-group-text"><i class="pe-7s-way btn-icon-wrapper"> </i></span></div>
                    </div>

                </div>
            </div>
            <div class="col-md-3"></div>
        </div>

        <div id="smartwizard-consultant-registration-view">


            <div class="card-header-custom  bg-moi-green">
                <i class="header-icon pe-7s-pen icon-gradient bg-white"> </i>
                @Localizer["consulting firm (cf) list"]
            </div>
            <div class="card-body">


                <table style="width: 100%;" id="tbl-Profile" name="tbl-Profile" class="table table-hover table-striped table-bordered">
                    <thead>
                        <tr>
                            <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-id btn-icon-wrapper"> </i> @Localizer["cr"]</th>
                            <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-culture btn-icon-wrapper"> </i> @Localizer["cf name"]</th>
                            <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-magnet btn-icon-wrapper"> </i> @Localizer["project name"]</th>
                            <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-magic-wand btn-icon-wrapper"> </i> @Localizer["contract start date"]</th>
                            <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-magic-wand btn-icon-wrapper"> </i> @Localizer["contract end date"]</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="text-center text-muted">1213526</td>
                            <td class="text-center">KPMG</td>
                            <td class="text-center">
                                al batnah coastal road
                            </td>
                            <td class="text-center">
                                11/11/2008
                            </td>
                            <td class="text-center">
                                11/11/2013
                            </td>
                        </tr>
                        <tr>
                            <td class="text-center text-muted">000786</td>
                            <td class="text-center">Al Ansari</td>
                            <td class="text-center">
                                al batnah coastal road
                            </td>
                            <td class="text-center">
                                11/11/2008
                            </td>
                            <td class="text-center">
                                11/11/2013
                            </td>
                        </tr>
                    </tbody>
                </table>

            </div>
            <div class="card-header-custom  bg-moi-green">
                <i class="header-icon pe-7s-pen icon-gradient bg-white"> </i>
                @Localizer["authorized user (for consulting firm (cf) select in above grid)"]
            </div>
            <div class="card-body">
                <table style="width: 100%;" id="tbl-ProfileUser" name="tbl-Profile" class="table table-hover table-striped table-bordered">
                    <thead>
                        <tr>
                            <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-id btn-icon-wrapper"> </i> @Localizer["national id"] </th>
                            <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-world btn-icon-wrapper"> </i> @Localizer["cf name"]</th>
                            <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-user btn-icon-wrapper"> </i> @Localizer["authorized user name"]</th>
                            <th class="bg-moi-gold" style="text-align:center"><i class="pe-7s-phone btn-icon-wrapper"> </i> @Localizer["mobile"]</th>
                        </tr>
                    </thead>
                    <tbody>
