
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model PCVMS.Model.Web.DashboardModel
<h1>Dashboard</h1>
<h1> list of Permissions</h1>
<div class="col-md-6">
    <table class="table">
        <tbody>
            <tr>
                <th style="width:20px">Number</th>
                <th>Name</th>
            </tr>
            @for (int i = 0; i < Model.PermissionList.Count; i++)
            {
                var item = Model.PermissionList[i];
                <tr>
                    <td>@(i + 1)</td>
                    <td>@item.Text</td>
                </tr>
            }
    </table>
</div>


<h1> list of RoleList</h1>
<div class="col-md-6">
    <table class="table">
        <tbody>
            <tr>
                <th style="width:20px">Number</th>
                <th>Name</th>
            </tr>
            @for (int i = 0; i < Model.RoleList.Count; i++)
            {
                var item = Model.RoleList[i];
                <tr>
                    <td>@(i + 1)</td>
                    <td>@item.Text</td>
                </tr>
            }
    </table>
</div>